{
  "apps": [
    {
      "name": "lord-obito-panel",
      "script": "app.js",
      "watch": true,
      "env": {
        "NODE_ENV": "development",
        "PORT": 3000
      },
      "env_production": {
        "NODE_ENV": "production",
        "PORT": 3000
      },
      "max_memory_restart": "300M",
      "instances": 1,
      "autorestart": true,
      "restart_delay": 5000,
      "out_file": "./logs/out.log",
      "error_file": "./logs/error.log",
      "log_date_format": "YYYY-MM-DD HH:mm:ss"
    }
  ]
}
